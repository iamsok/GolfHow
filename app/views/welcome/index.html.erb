<h3>Hello Hudl</h3>
<% if user_signed_in? %>
  <%= form_tag(root_path, :method => "get", id: "search-form") do %>
    <%= text_field_tag :search, params[:search], placeholder: "Search", class: "radius", name: 'search' %>
    <%= submit_tag "Search", class: "button radius", :title => nil %>
  <% end %>
<% end %>

<embed width="420" height="315" src="<%= @video_url %>">
